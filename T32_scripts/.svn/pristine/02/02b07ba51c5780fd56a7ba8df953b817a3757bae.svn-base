block_fuse_show_gui:		
	&i=0.
	while (&i<=63.)
	(
		&BLOCK_DATA=FORMAT.Decimal(2.,&i)
		&BLOCK_DATA=STRing.Replace("&BLOCK_DATA"," ","0",0.)
		&NAME="&BLOCK_NAME"+"&BLOCK_DATA"
		&READ_DATA="0x"+string.mid("&WRITE_DATA",(63.*8.-8.*&i),8.)
		DIALOG.Set &NAME "&READ_DATA"
		;DIALOG.DISABLE &NAME
		&i=&i+1.
	)
	STOP
		
