	DIALOG.END
	RETURN
Load_block_fuse:
	OPEN #1 &cur_path\block_all_fuse_value.txt /Read
	READ #1 %line &WRITE_DATA
	CLOSE #1
	Gosub block_fuse_show_gui
cancel:
	DIALOG.END
	&DC_TYPE="READ"
	gosub efuse_recovery
